; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Character",
"language": "GDScript",
"path": "res://src/scripts/Character.gd"
}, {
"base": "Resource",
"class": "CharacterType",
"language": "GDScript",
"path": "res://src/entities/character_type.gd"
}, {
"base": "Resource",
"class": "Item",
"language": "GDScript",
"path": "res://src/entities/item.gd"
}, {
"base": "Resource",
"class": "Quest",
"language": "GDScript",
"path": "res://src/entities/quest.gd"
} ]
_global_script_class_icons={
"Character": "",
"CharacterType": "",
"Item": "",
"Quest": ""
}

[application]

config/name="Quest Please"
run/main_scene="res://src/scenes/Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="questplease"
config/icon="res://icon.png"

[autoload]

QuestLoader="*res://src/globals/quest_loader.gd"
CharacterLoader="*res://src/globals/character_loader.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[layer_names]

2d_render/layer_1="miniature"
2d_render/layer_2="details"

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
